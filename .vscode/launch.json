{
    "configurations": [
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch-RPi_local",
            "target": "${workspaceFolder}/Builds/DroneExec",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "(gdb) Thread_Manager_UT",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/SW_components/services/Thread_Manager_UT/builds_UT/Thread_Manager_UT",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Włącz formatowanie kodu dla gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Ustaw wariant dezasemblacji na Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
   ]
}