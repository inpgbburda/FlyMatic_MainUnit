{
    "configurations": [

    {
        "name": "(gdb) Launch-RPi_local",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/Builds/executable",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "environment": [],
        // "miDebuggerPath": "${workspaceFolder}/Tools/sudo_gdb/sudo_gdb.sh",
        "MIMode": "gdb",
        "setupCommands": 
        [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        ]
